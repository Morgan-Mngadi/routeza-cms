{
  "kind": "collectionType",
  "collectionName": "content_tags",
  "info": {
    "singularName": "content-tag",
    "pluralName": "content-tags",
    "displayName": "content-tags"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true,
      "unique": true
    },
    "blogPosts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::blog-post.blog-post",
      "mappedBy": "tags"
    },
    "newsArticles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::news-article.news-article",
      "mappedBy": "tags"
    }
  }
}
